{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch MREify Package",
        "runtimeExecutable": "npm",
        "runtimeArgs": [
          "run-script",
          "debug"
        ],
        "port": 9229,
        "cwd": "${workspaceFolder}",
        "internalConsoleOptions": "openOnSessionStart",
        "autoAttachChildProcesses": true,
        "console": "internalConsole",
        "outputCapture": "std"
      },
    ]
  }